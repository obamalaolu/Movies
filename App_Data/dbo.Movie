CREATE TABLE [dbo].[Movie] (
    [Id]          INT              IDENTITY (1, 1) NOT NULL,
    [title]       VARCHAR (100)    NOT NULL,
    [releaseDate] DATE             NULL,
    [genre]       VARCHAR (20)     NULL,
    [price]       FLOAT (53)       NOT NULL,
    [images]      VARCHAR(MAX) NULL,
    PRIMARY KEY CLUSTERED ([Id] ASC)
);

